<?xml version="1.0"?>
<launch>
    <!-- We resume the logic in empty_world.launch, changing only Atom One Darkthe name of the world to be launched -->
    <include file="$(find gazebo_ros)/launch/empty_world.launch">
        <arg name="world_name" value="$(find ddr_gazebo)/worlds/robotarium.world"/>
        <!-- more default parameters can be changed here -->
        <param name="/use_sim_time" value="false"/>
    </include>

</launch>
